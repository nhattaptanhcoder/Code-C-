#include <stdio.h>

void checkQPrime(int n, int arrQPrime[], int *size){
    int k = 0;
    for (int i = 5; i <= n; i++) {
        int count = 0;
        for (int j = 2; j <= n / 2; j++) {
            if (i % j == 0) {
                count++;
            }
        }
        if (count == 2) {
            arrQPrime[k] = i;
            k++;
        }
    }
    *size = k;
}

void printQPrime(int checkedArr[], int size) {
    for (int i = 0; i < size; i++) {
        printf("%d ", checkedArr[i]);
    }
    printf("\n");
}

int main() {
    int n;
    int arrQPrime[1000]; // cấp phát trước mảng lưu kết quả
    int size = 0;

    printf("Nhap n: ");
    scanf("%d", &n);

    checkQPrime(n, arrQPrime, &size);
    printQPrime(arrQPrime, size);

    return 0;
}
